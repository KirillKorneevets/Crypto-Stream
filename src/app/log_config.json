{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "standard": {
        "format": "%(asctime)s [%(levelname)s] %(name)s: %(message)s"
      }
    },
    "handlers": {
      "file": {
        "level": "DEBUG",
        "class": "logging.FileHandler",
        "filename": "app.log",
        "formatter": "standard"
      }
    },
    "loggers": {
      "": {
        "handlers": ["file"],
        "level": "DEBUG",
        "propagate": true
      }
    }
}